<table xml:id="table-eqns-const-lin-a">
  <title>Special equations for constant (translational) acceleration</title>
  <tabular top="major" left="major" halign="center">
    <col right="medium" width="40%"/>
    <col right="major" width="40%"/>
    <row header="yes" valign="top" bottom="medium">
      <cell>Usefulness</cell>
      <cell>Special-case Equation</cell>
    </row>
    <row valign="top" bottom="minor">
      <cell>"average velocity eqn" </cell>
      <cell><m>\vec{v}_{\text{ave}}=\frac{ \vec{v}_{\text{initial}} + \vec{v}_{\text{final}} }{2}  </m></cell>
    </row>
    <row valign="top" bottom="minor">
      <cell>"velocity eqn"</cell>
      <cell><m>\vec{v}_{\text{final}}= \vec{v}_{\text{initial}} + \vec{a}\cdot t </m></cell>
    </row>
    <row valign="top" bottom="minor">
      <cell>"displacement eqn"</cell>
      <cell><m>\Delta \vec{x} = \vec{v}_{\text{initial}}\cdot t + \frac{1}{2} \vec{a}\cdot t^2 </m></cell>
    </row>
    <row valign="top" bottom="major">
      <cell>"braking distance eqn"</cell>
      <cell><m>\vec{v}_{\text{final}}^2= \vec{v}_{\text{initial}}^2 + 2 \vec{a}\cdot \Delta\vec{x}</m></cell>
    </row>
  </tabular>
