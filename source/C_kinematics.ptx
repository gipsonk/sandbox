  <table xml:id="table-defns-lin-kin" >
  <title>Translational Kinematics Definitions</title>
  <tabular top="major" left="major" halign="center">
    <col right="medium" width="40%"/>
    <col right="major" width="40%"/>
    <row header="yes" valign="top" bottom="medium">
      <cell>Quantity</cell>
      <cell>Definition</cell>
    </row>
    <row valign="top" bottom="minor">
      <cell>displacement </cell>
      <cell><m>\Delta \vec{x}=\vec{x}_{\text{final}}-\vec{x}_{\text{initial}} </m></cell>
